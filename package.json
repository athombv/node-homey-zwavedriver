{
  "name": "homey-zwavedriver",
  "version": "2.0.2",
  "description": "This module can be used to make the development of Z-Wave apps for Homey easier.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive",
    "lint": "eslint .",
    "serve": "concurrently \"serve build/docs/\" \"npm run jsdoc:watch\"",
    "build": "npm run jsdoc",
    "jsdoc": "cd docs; jsdoc -c jsdoc.json",
    "jsdoc:watch": "npm-watch jsdoc"
  },
  "keywords": [
    "zwave",
    "homey"
  ],
  "author": "Athom B.V.",
  "license": "ISC",
  "dependencies": {
    "color-space": "^1.16.0"
  },
  "devDependencies": {
    "braintree-jsdoc-template": "^3.3.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-athom": "^2.0.6",
    "jsdoc": "^3.6.4",
    "npm-watch": "^0.6.0",
    "serve": "^11.3.1",
    "homey": "^2.1.5",
    "mocha": "^7.1.1"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/athombv/node-homey-zwavedriver.git"
  },
  "bugs": {
    "url": "https://github.com/athombv/node-homey-zwavedriver/issues"
  },
  "homepage": "https://github.com/athombv/node-homey-zwavedriver#readme",
  "watch": {
    "jsdoc": {
      "patterns": [
        "lib"
      ],
      "extensions": [
        "js"
      ]
    }
  }
}
